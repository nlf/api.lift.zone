{
  "aws": {
    "ses": {
      "accessKeyId": "$AWS_ACCESS_KEY",
      "secretAccessKey": "$AWS_SECRET_ACCESS_KEY"
    }
  },
  "clientUri": "$CLIENT_URI",
  "db": {
    "client": "postgresql",
    "connection": "$DATABASE_URL"
  },
  "auth": {
    "secret": "$AUTH_SECRET",
    "seedPassword": "$SEED_PASSWORD"
  },
  "hapi": {
    "port": 3001,
    "cache": {
      "engine": "catbox-memory"
    },
    "routes": {
      "cors": {
        "additionalExposedHeaders": ["Link"],
        "origin": ["$CORS_ORIGIN"]
      }
    }
  },
  "good": {
    "reporters": {
      "console": [{
        "module": "good-squeeze",
        "name": "Squeeze",
        "args": [{
          "log": "*",
          "response": "*",
          "request": "*",
          "error": "*"
        }]
      }, {
        "module": "good-console"
      }, "stdout"
      ]
    }
  },
  "pagination": {
    "meta": {
      "baseUri": "$API_URI"
    }
  }
}
