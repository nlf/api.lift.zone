{
  "aws": {
    "ses": {
      "region": "us-east-1"
    }
  },
  "email": {
    "from": "Lift zone <support@lift.zone>"
  },
  "auth": {
    "options": {
      "algorithm": "HS256"
    }
  },
  "saltRounds": 10,
  "tokenLength": 48,
  "pino": {
    "prettyPrint": true
  },
  "rateLimit": {
    "userLimit": false,
    "pathLimit": false
  },
  "inviteValidateRateLimit": {
    "pathLimit": 1,
    "pathCache": {
      "expiresIn": 10
    }
  },
  "invites": {
    "count": 5
  },
  "shutdownTimeout": 15000,
  "pagination": {
    "query": {
      "page": {
        "default": 0
      }
    },
    "meta": {
      "successStatusCode": 206,
      "location": "header",
      "last": {
        "active": false
      },
      "first": {
        "active": false
      }
    },
    "routes": {
      "include": ["/activities", "/activities/{id}/history"]
    }
  }
}
