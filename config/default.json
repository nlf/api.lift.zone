{
  "aws": {
    "ses": {
      "region": "us-west-2"
    }
  },
  "email": {
    "from": "Lift zone <support@lift.zone>"
  },
  "auth": {
    "options": {
      "algorithm": "HS256"
    }
  },
  "saltRounds": 10,
  "tokenLength": 48,
  "good": {
    "includes": {
      "request": ["headers"]
    }
  },
  "rateLimit": {
    "userLimit": false,
    "pathLimit": false
  },
  "inviteValidateRateLimit": {
    "pathLimit": 1,
    "pathCache": {
      "expiresIn": 10
    }
  },
  "invites": {
    "count": 5
  }
}
