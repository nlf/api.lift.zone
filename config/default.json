{
  "auth": {
    "options": {
      "algorithm": "HS256"
    }
  },
  "saltRounds": 10,
  "tokenLength": 48,
  "hapi": {
    "connections": {
      "routes": {
        "security": true,
        "cors": true
      }
    }
  },
  "good": {
    "includes": {
      "request": ["headers"]
    }
  },
  "rateLimit": {
    "userLimit": false,
    "pathLimit": false
  },
  "inviteValidateRateLimit": {
    "pathLimit": 1,
    "pathCache": {
      "expiresIn": 10
    }
  },
  "invites": {
    "count": 5
  }
}
